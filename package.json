{
  "name": "vietvip-web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vite serve",
    "serve:prod": "vite serve --mode production",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:uat": "vite build --mode uat",
